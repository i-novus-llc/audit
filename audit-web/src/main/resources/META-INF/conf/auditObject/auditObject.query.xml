<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0" name="Тип события">
    <list count-mapping="[totalElements]" result-mapping="[content]">
        <rest filters-separator="&amp;">${audit.backend.url}/audit/objects?{filters}&amp;size={limit}&amp;page={page}</rest>
    </list>
    <fields>
        <field id="id" domain="string">
            <select/>
        </field>
        <field id="name" domain="string">
            <select/>
            <sorting/>
            <filters>
                <eq filter-id="name" mapping="['name']">name={name}</eq>
            </filters>
        </field>
    </fields>
</query>

