<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0" name="Система/Модуль">
    <list>
        <rest>${audit.backend.url}/audit/sourceApplication/</rest>
    </list>
    <fields>
        <field id="id" domain="string">
            <select/>
            <filters>
                <eq filter-id="id">:id</eq>
            </filters>
        </field>
        <field id="name" domain="string">
            <select/>
            <sorting/>
            <filters>
                <eq filter-id="name" mapping="['name']">name={name}</eq>
            </filters>
        </field>
    </fields>
</query>

